{
    "name": "auvipev/viper-engine",
    "description": "Viper: Web engine for Viper Content Management System.",
    "license": "MIT",
    "keywords": [
        "php",
        "viper",
        "engine-management",
        "elegant-coding",
        "secure-by-default"
    ],
    "authors": [
        {
            "name": "Nicholas English",
            "email": "nenglish6657@gmail.com",
            "homepage": "https://github.com/Auvipev"
        },
        {
            "name": "Viper Contributors",
            "homepage": "https://github.com/Auvipev/Engine/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=7.2",
        "psr/log": "^1.0",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "psr/simple-cache": "^1.0",
        "psr/http-server-handler": "^1.0",
        "paragonie/ciphersweet": "^1.1",
        "paragonie/certainty": "2.1",
        "symfony/polyfill-php71": "^1.8",
        "symfony/polyfill-php72": "^1.8",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-sodium": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "vimeo/psalm": "^2.0",
        "phpmd/phpmd": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Viper\\Engine\\": "./src"
        },
        "files": [
            "./bin/version.php",
            "./bin/bootstrap.php"
        ]
    }
}
