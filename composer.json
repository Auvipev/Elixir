{
    "name": "auvipev/viper",
    "description": "Viper: A secure by default content management system.",
    "license": "MIT",
    "keywords": [
        "php",
        "viper",
        "content-management-system",
        "secure-by-default"
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-sodium": "*"
    },
    "authors": [
        {
            "name": "Nicholas English",
            "email": "nenglish443@outlook.com",
            "homepage": "https://github.com/Auvipev"
        },
        {
            "name": "Viper Contributors",
            "homepage": "https://github.com/Auvipev/Viper/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Auvipev\\Viper\\": "./src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "vimeo/psalm": "^2.0"
    },
    "config": {
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
