{
    "name": "auvipev/viper-engine",
    "description": "Viper: A secure by default web engine making websites easy to build.",
    "license": "MIT",
    "keywords": [
        "php",
        "viper",
        "engine-management",
        "secure-by-default"
    ],
    "authors": [
        {
            "name": "Viper Contributors",
            "homepage": "https://github.com/Auvipev/Viper-Engine/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=7.0",
        "psr/log": "^1.0",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "psr/simple-cache": "^1.0",
        "psr/http-server-handler": "^1.0",
        "paragonie/ciphersweet": "^1.1",
        "paragonie/certainty": "2.1",
        "symfony/polyfill-php71": "^1.8",
        "symfony/polyfill-php72": "^1.8",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-sodium": "*",
        "ext-openssl": "*",
        "ext-tokenizer": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.1",
        "phpmd/phpmd": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Auvipev\\Viper\\": "./src"
        },
        "files": [
            "./bin/version.php",
            "./bin/bootstrap.php"
        ]
    }
}
